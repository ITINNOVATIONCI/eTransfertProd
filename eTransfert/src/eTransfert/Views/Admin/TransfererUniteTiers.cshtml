                        </div>
                        <div class="tools">
                            @*<a href="javascript:;" class="collapse">
                            </a>
                            <a href="#portlet-config" data-toggle="modal" class="config">
                            </a>*@
                            <a href="javascript:;" class="reload">
                            </a>
                            @*<a href="javascript:;" class="remove">
                            </a>*@
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <!-- BEGIN FORM-->
                        
                            <form asp-action="PaiementUniteTiers" asp-controller="Home" class="form-horizontal form-bordered" >
                                <div class="form-body">
                                    <div class="form-group">
                                        <label class="control-label col-md-3">Email</label>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" readonly value="@ViewBag.email" name="email" id="email">

                                        </div>
                                    </div>

                                    <input type="hidden" value="@ViewBag.id" id="idUtilisateur" name="idUtilisateur" />

                                    <div class="form-group">
                                        <label class="control-label col-md-3">Num&eacute;ro</label>
                                        <div class="col-md-4">
                                            <input class="form-control" id="Numero" name="Numero" required type="text" />
                                            @*<span class="help-block">
                            99-99-99-99
                        </span>*@
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label col-md-3">Montant</label>
                                        <div class="col-md-4">
                                            <input class="form-control" id="Montant" name="Montant" required type="text" />
                                            <span class="help-block">
                                                @*mask "9" or mask "99" or ... mask "9999999999"*@
                                            </span>
                                        </div>
                                    </div>

                                    <div class="form-group ">
                                        <label class="control-label col-md-3">Type de transfert</label>

                                        <div class="col-md-6">

                                            <div class="md-radio-inline">
                                                <div class="md-radio">
                                                    <input type="radio" id="radio14" value="RAPIDE" name="TypeTransfert" class="md-radiobtn">
                                                    <label for="radio14">
                                                        <span></span>
                                                        <span class="check"></span>
                                                        <span class="box"></span>
                                                        RAPIDE
                                                    </label>
                                                </div>
                                                <div class="md-radio has-error">
                                                    <input type="radio" id="radio15" name="TypeTransfert" value="COMPTE" class="md-radiobtn">
                                                    <label for="radio15">
                                                        <span></span>
                                                        <span class="check"></span>
                                                        <span class="box"></span>
                                                        COMPTE
                                                    </label>
                                                </div>
                                                <div class="md-radio has-warning">
                                                    <input type="radio" id="radio16" name="TypeTransfert" value="VIP" class="md-radiobtn" checked>
                                                    <label for="radio16">
                                                        <span></span>
                                                        <span class="check"></span>
                                                        <span class="box"></span>
                                                        VIP
                                                    </label>
                                                </div>

                                                <div class="md-radio">
                                                    <input type="radio" id="radio17" value="SUPERVIP" name="TypeTransfert" class="md-radiobtn">
                                                    <label for="radio17">
                                                        <span></span>
                                                        <span class="check"></span>
                                                        <span class="box"></span>
                                                        SUPER VIP
                                                    </label>
                                                </div>

                                                <div class="md-radio">
                                                    <input type="radio" id="radio18" value="ADMIN" name="TypeTransfert" class="md-radiobtn">
                                                    <label for="radio18">
                                                        <span></span>
                                                        <span class="check"></span>
                                                        <span class="box"></span>
                                                        ADMIN
                                                    </label>
                                                </div>
                                                </div>
                                            </div>

                                        </div>

                                    </div>
                                <div class="form-actions">
                                    <div class="row">
                                        <div class="col-md-offset-3 col-md-9">
                                            <button type="submit" class="btn green"><i class="fa fa-check"></i> Valider</button>
                                            @*<button type="button" class="btn default">Cancel</button>*@
                                        </div>
                                    </div>
                                </div>
                            </form>
                        <!-- END FORM-->
                    </div>
                </div>





                <div style="font-size:large;">
                    <a asp-action="ListeUtilisateur">Revenir &agrave; la Liste</a>
                </div>




